{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/episodes/(?<episodeId>[^/]+)",
      "dest": "/api/episodes?episodeId=$episodeId"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/subscriber-dashboard.html",
      "dest": "/public/subscriber-dashboard.html"
    },
    {
      "src": "/subscriber-login.html",
      "dest": "/public/subscriber-login.html"
    },
    {
      "src": "/subscriber-create-account.html",
      "dest": "/public/subscriber-create-account.html"
    },
    {
      "src": "/dashboard",
      "dest": "/public/subscriber-dashboard.html"
    },
    {
      "src": "/login",
      "dest": "/public/subscriber-login.html"
    },
    {
      "src": "/create-account",
      "dest": "/public/subscriber-create-account.html"
    },
    {
      "src": "/search.html",
      "dest": "/public/search.html"
    },
    {
      "src": "/episodes/(?<episodeId>[^/]+)",
      "dest": "/api/episodes?episodeId=$episodeId"
    },
    {
      "src": "/create",
      "status": 307,
      "headers": {
        "Location": "https://storage.googleapis.com/regal-scholar-453620-r7-podcast-storage/form.html?redirect=true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
} 